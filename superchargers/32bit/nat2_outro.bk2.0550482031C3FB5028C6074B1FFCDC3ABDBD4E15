                  var direction = ATVI.pageLocale == "ar" ? true : false;

                  $parentContainer.slick({ 
                      slidesToShow: 1,
                      slidesToScroll: 1,
					  infinite: true, 
					  speed: 300,
                      dots: true,
                      centerMode: false,
                      variableWidth: false,
                      adaptiveHeight: false,
                      centerPadding: "0px",
                      arrows: true,
                      autoplay: false,
                      autoplaySpeed: 5000,
                      fade: false,
                      rtl: direction,
                      responsive: [
                            {
                                breakpoint: 0,
                                settings: {
									slidesToShow: 1,
                                    slidesToScroll: 1,
					  				infinite: true, 
									speed: 300,
                                    dots: false,
